{
  "extends": "../wix-code-types/configs/tsconfig.page",
  "compilerOptions": {
    "composite": true,
    "noEmit": false,
    "tsBuildInfoFile": ".tsbuildinfo",
    "baseUrl": ".",
    "jsx": "react",
    "paths": {
      "public/*": [
        "../../../src/public/*"
      ],
      "backend/*.web.js": [
        "../../../src/backend/*.web.js"
      ],
      "backend/*.web": [
        "../../../src/backend/*.web.js"
      ]
    },
    "outDir": "build",
    "rootDir": "../../.."
  },
  "files": [
    "../../../src/pages/Portfolio.nukk7.js"
  ],
  "include": [
    "nukk7.d.ts",
    "../../../src/public/**/*",
    "../../../src/backend/**/*.web.js",
    "../backend.d.ts"
  ]
}
